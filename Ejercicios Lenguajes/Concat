//Concat 1

const num1 = [1, 2, 3];
const num2 = [4, 5, 6];
var concatFun=(arr1,arr2) =>console.log(...arr1,...arr2);
concatFun(num1,num2);

//concat optional
const num1 = [1, 2, 3];
const num2 = [4, 5, 6];
const num3 = [7, 8, 9];
const num4 = [10, 11, 12];

var concatFun2 = (arr1,arr2,...arr3) => 
{
    var cadena1=new Array;
    arr3.forEach(function(element){
        cadena1= cadena1.concat(element);
      });
console.log(...arr1,...arr2,...cadena1)
}

concatFun2(num1,num2,num3,num4);
